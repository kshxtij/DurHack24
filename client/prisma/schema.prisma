datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}


model Automation {
  id       Int      @id @default(autoincrement())
  title     String
  service  String
  severity String
  cron     String
  email   String
}

model Alert {
  id          Int      @id @default(autoincrement())
  email       String
  createdAt   DateTime @default(now())
  service  String
  severity String
}
